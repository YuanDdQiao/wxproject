checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    color: #FFBE00!important;
    background-color: #FFBE00!important;
    border-radius: 50%;
}

checkbox .wx-checkbox-input.wx-checkbox-input {
    border-radius: 50%;
    background-color: #F6F6F6;
}

checkbox {
    transform: scale(.5);
}

label {
    margin-bottom: 0;
}

.form-check-label {
    margin-right: 15rpx;
}

.form-group {
    margin-bottom: 0;
    margin-left: 40rpx;
    margin-right: 40rpx;
}

.ppg_bottom_border {
    border-bottom: 1rpx solid #E8E8E8;
}

.ppg_infoHeight {
    height: 91rpx;
    line-height: 130rpx;
    font-size: 32rpx;
    letter-spacing: 1.6rpx;
}

.ppg_left {
    color: #3A3C4C;
}

.ppg_right {
    color: #3A3C4C;
}

input {
    display: inline-block;
    min-height: 0;
    overflow: visible;
}

.addPet {
    position: absolute;
    top: 0;
    right: 72rpx;
    font-size: 26rpx;
    color: #576B95;
    letter-spacing: 1.3rpx;
    z-index: 9999;
}

.addPet image {
    width: 42rpx;
    height: 42rpx;
    border-radius: 50%;
    vertical-align: -8%;
}

checkbox-group {
    display: inline-block;
}

.form-check-inline+.form-check-inline {
    margin-left: 0;
}

.ppg_confirm {
    width: 658rpx;
    height: 90rpx;
    line-height: 90rpx;
    position: absolute;
    left: 50%;
    bottom: 45rpx;
    transform: translate(-50%);
    background: #DBDFE4;
    box-shadow: 0 1rpx 1rpx 0 rgba(0,0,0,0.10);
    border-radius: 100rpx;
    color: #fff;
}

.ppg_over {
    width: 658rpx;
    height: 90rpx;
    line-height: 90rpx;
    position: absolute;
    left: 50%;
    bottom: 45rpx;
    transform: translate(-50%);
    background: #FFBE00;
    box-shadow: 0 1rpx 1rpx 0 rgba(0,0,0,0.10);
    border-radius: 100rpx;
    color: #fff;
}

.ppg_petConfirm {
    width: 576rpx;
    height: 90rpx;
    margin: 0 auto;
    background: #FFBE00;
    border-radius: 100rpx;
    font-size: 36rpx;
    color: #FFFFFF;
    margin-bottom: 40rpx;
    text-align: center;
    line-height: 90rpx;
}

radio {
    position: absolute;
    top: 50%;
    transform: scale(.7) translateY(-50%);
    right: 0;
}

radio .wx-radio-input.wx-radio-input-checked {
    color: #FFBE00!important;
    background-color: #FFBE00!important;
    border: none;
}

radio .wx-radio-input.wx-radio-input-checked::before {
    color: #FFBE00!important;
}

radio .wx-radio-input.wx-radio-input {
    background-color: #F6F6F6;
}

.label {
    width: 100%;
    height: 100%;
}

.mask {
    position: absolute;
    top: 0;
    z-index: 9999;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.6);
}

.petSelect {
    position: absolute;
    z-index: 99999;
    width: 100%;
    background-color: #f6f6f6;
    bottom: 0;
    animation: fadeIn 1s;
}

.top {
    height: 82rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
    line-height: 82rpx;
    padding-left: 22rpx;
    padding-right: 22rpx;
}

.top .title {
    font-size: 32rpx;
    color: #898989;
    display: inline-block;
}

.top .cancel {
    font-size: 26rpx;
    color: #576B95;
    display: inline-block;
    float: right;
}

.petSelect .pet {
    margin-bottom: 87rpx;
}

.petSelect .pet .petInfo {
    position: relative;
    background-color: #fff;
    height: 156rpx;
    margin-bottom: 20rpx;
    padding-left: 22rpx;
    padding-right: 22rpx;
    overflow: hidden;
}

.petSelect .pet .petInfo .headPic {
    width: 110rpx;
    height: 110rpx;
    background-color: #f6f6f6;
    margin-top: 23rpx;
    border-radius: 50%;
}

.petSelect .pet .petInfo .headPic image {
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
}

.petSelect .pet .petInfo .petName {
    left: 152rpx;
    top: 38rpx;
    position: absolute;
    font-size: 32rpx;
    color: #3A3C4C;
}

.petSelect .pet .petInfo .petSex .sexSelect {
    position: absolute;
    left: 152rpx;
    top: 93rpx;
    font-size: 22rpx;
    color: #898989;
}

.petSelect .pet .petInfo .petSex image {
    width: 12rpx;
    height: 21rpx;
}